{
  "name": "jsii",
  "version": "0.0.0",
  "description": "TypeScript compiler for jsii",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "homepage": "https://github.com/aws/jsii",
  "bugs": {
    "url": "https://github.com/aws/jsii/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/jsii"
  },
  "engines": {
    "node": ">= 12.7.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii": "bin/jsii"
  },
  "scripts": {
    "gen": "cp ../../README.md . && bash ./generate.sh",
    "build": "yarn gen && tsc --build && npm run lint",
    "watch": "bash ./generate.sh && tsc --build -w",
    "lint": "eslint . --ext .js,.ts --ignore-path=.gitignore --ignore-pattern=test/negatives/*",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:update": "jest -u",
    "package": "package-js"
  },
  "dependencies": {
    "@jsii/check-node": "0.0.0",
    "@jsii/spec": "^0.0.0",
    "case": "^1.6.3",
    "chalk": "^4",
    "deep-equal": "^2.0.5",
    "fs-extra": "^9.1.0",
    "log4js": "^6.4.3",
    "semver": "^7.3.5",
    "semver-intersect": "^1.4.0",
    "sort-json": "^2.0.1",
    "spdx-license-list": "^6.4.0",
    "typescript": "~3.9.10",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/clone": "^2.1.1",
    "@types/deep-equal": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.20.47",
    "@types/semver": "^7.3.9",
    "clone": "^2.1.2",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "jest-expect-message": "^1.0.2",
    "jsii-build-tools": "^0.0.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3"
  }
}
