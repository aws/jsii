{
  "name": "jsii-config",
  "version": "0.0.0",
  "description": "CLI tool for configuring jsii module configuration in package.json",
  "main": "lib/index.js",
  "repository": "https://github.com/aws/jsii",
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --build && npm run lint",
    "watch": "tsc --build -w",
    "lint": "eslint . --ext .js,.ts --ignore-path=.gitignore --ignore-pattern=test/negatives/*",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "package": "package-js",
    "exec": "npm run build && node bin/jsii-config.js"
  },
  "bin": {
    "jsii-config": "bin/jsii-config"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.20.43",
    "@types/yargs": "^17.0.8",
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "jest-expect-message": "^1.0.2",
    "prettier": "^2.5.1",
    "typescript": "~3.9.10"
  },
  "dependencies": {
    "@jsii/check-node": "0.0.0",
    "@jsii/spec": "^0.0.0",
    "inquirer": "^7.3.3",
    "yargs": "^16.2.0"
  }
}
