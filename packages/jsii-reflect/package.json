{
  "name": "jsii-reflect",
  "version": "0.0.0",
  "description": "strongly-typed reflection library and tools for jsii",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "homepage": "https://github.com/aws/jsii",
  "bugs": {
    "url": "https://github.com/aws/jsii/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/jsii-reflect"
  },
  "engines": {
    "node": ">= 10.3.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii-tree": "bin/jsii-tree"
  },
  "scripts": {
    "build": "tsc --build && chmod +x bin/jsii-tree && npm run lint",
    "watch": "tsc --build -w",
    "lint": "eslint . --ext .js,.ts --ignore-path=.gitignore",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:update": "jest -u",
    "package": "package-js"
  },
  "dependencies": {
    "@jsii/spec": "^0.0.0",
    "colors": "^1.4.0",
    "fs-extra": "^9.0.1",
    "oo-ascii-tree": "^0.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@scope/jsii-calc-lib": "^0.0.0",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^26.0.9",
    "@types/node": "^10.17.28",
    "@types/yargs": "^15.0.5",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "jsii": "^0.0.0",
    "jsii-build-tools": "^0.0.0",
    "jsii-calc": "^0.0.0",
    "prettier": "^2.0.5",
    "typescript": "~3.9.7"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/bin/**/*.js",
      "**/lib/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "statements": 45
      }
    },
    "errorOnDeprecated": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).js"
    ]
  }
}
