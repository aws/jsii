    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:update": "jest -u",
    "package": "package-js"
  },
  "dependencies": {
    "@jsii/spec": "^0.0.0",
    "colors": "^1.4.0",
    "fs-extra": "^9.0.1",
