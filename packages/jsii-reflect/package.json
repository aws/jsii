    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@scope/jsii-calc-lib": "^0.0.0",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^10.17.51",
    "@types/yargs": "^15.0.12",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "jsii": "^0.0.0",
    "jsii-build-tools": "^0.0.0",
