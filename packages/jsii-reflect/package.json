  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/jsii-reflect"
  },
  "engines": {
    "node": ">= 10.3.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii-tree": "bin/jsii-tree"
