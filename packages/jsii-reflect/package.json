{
  "name": "jsii-reflect",
  "version": "0.16.0",
  "description": "strongly-typed reflection library and tools for jsii",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii-tree": "bin/jsii-tree"
  },
  "scripts": {
    "build": "tsc --build && chmod +x bin/jsii-tree",
    "watch": "tsc --build -w",
    "test": "jest",
    "package": "package-js"
  },
  "dependencies": {
    "colors": "^1.3.3",
    "fs-extra": "^8.1.0",
    "jsii-spec": "^0.16.0",
    "oo-ascii-tree": "^0.16.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@scope/jsii-calc-lib": "file:../jsii-calc-lib",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/yargs": "^13.0.2",
    "jest": "^24.9.0",
    "jsii": "^0.16.0",
    "jsii-build-tools": "file:../jsii-build-tools",
    "jsii-calc": "file:../jsii-calc",
    "ts-jest": "^24.1.0",
    "typescript": "~3.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/{bin,lib}/**/*.js",
      "!**/*.d.ts"
    ],
    "coverageReporters": [
      "text-summary",
      "html",
      "lcov"
    ]
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/jsii-reflect"
  }
}
