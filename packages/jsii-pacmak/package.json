{
  "name": "jsii-pacmak",
  "version": "0.8.2",
  "description": "A code generation framework for jsii backend languages",
  "bin": {
    "jsii-pacmak": "bin/jsii-pacmak"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "gen": "/bin/bash generate.sh",
    "build": "npm run gen && tsc && chmod +x bin/jsii-pacmak && tslint -p .",
    "watch": "tsc -w",
    "lint": "tslint -p . --force",
    "test": "/bin/bash test/diff-test.sh && /bin/bash test/build-test.sh",
    "package": "package-js"
  },
  "keywords": [
    "jsii",
    "aws"
  ],
  "dependencies": {
    "clone": "^2.1.2",
    "codemaker": "^0.8.2",
    "escape-string-regexp": "^1.0.5",
    "fs-extra": "^7.0.1",
    "jsii-spec": "^0.8.2",
    "spdx-license-list": "^5.0.0",
    "xmlbuilder": "^11.0.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@scope/jsii-calc-lib": "^0.8.2",
    "@types/clone": "^0.1.30",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^8.10.43",
    "@types/nodeunit": "^0.0.30",
    "@types/xmlbuilder": "^0.0.34",
    "@types/yargs": "^12.0.9",
    "jsii-build-tools": "^0.8.2",
    "jsii-calc": "^0.8.2",
    "jsii-dotnet-generator": "^0.8.2",
    "jsii-dotnet-jsonmodel": "^0.8.2",
    "jsii-dotnet-runtime": "^0.8.2",
    "jsii-java-runtime": "^0.8.2",
    "nodeunit": "^0.11.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/jsii.git"
  }
}
