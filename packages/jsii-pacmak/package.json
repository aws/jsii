{
  "name": "jsii-pacmak",
  "version": "0.10.0",
  "description": "A code generation framework for jsii backend languages",
  "bin": {
    "jsii-pacmak": "bin/jsii-pacmak"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "gen": "/bin/bash generate.sh",
    "build": "npm run gen && tsc --build && chmod +x bin/jsii-pacmak && tslint -p .",
    "watch": "tsc --build -w",
    "lint": "tslint -p . --force",
    "test": "/bin/bash test/diff-test.sh && /bin/bash test/build-test.sh",
    "package": "package-js"
  },
  "keywords": [
    "jsii",
    "aws"
  ],
  "dependencies": {
    "clone": "^2.1.2",
    "codemaker": "^0.10.0",
    "escape-string-regexp": "^2.0.0",
    "fs-extra": "^7.0.1",
    "jsii-spec": "^0.10.0",
    "spdx-license-list": "^6.0.0",
    "xmlbuilder": "^12.0.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@scope/jsii-calc-lib": "^0.10.0",
    "@types/clone": "^0.1.30",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/nodeunit": "^0.0.30",
    "@types/xmlbuilder": "^11.0.1",
    "@types/yargs": "^13.0.0",
    "jsii-build-tools": "^0.10.0",
    "jsii-calc": "^0.10.0",
    "jsii-dotnet-generator": "^0.10.0",
    "jsii-dotnet-jsonmodel": "^0.10.0",
    "jsii-dotnet-runtime": "^0.10.0",
    "jsii-java-runtime": "^0.10.0",
    "nodeunit": "^0.11.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.3"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/jsii.git",
    "directory": "packages/jsii-pacmak"
  }
}
