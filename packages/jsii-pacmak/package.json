{
  "name": "jsii-pacmak",
  "version": "0.5.0-beta",
  "description": "A code generation framework for jsii backend languages",
  "bin": {
    "jsii-pacmak": "bin/jsii-pacmak"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "bash generate.sh && tsc && chmod +x bin/jsii-pacmak && tslint -p .",
    "watch": "tsc -w",
    "lint": "tslint -p . --force",
    "test": "/bin/bash test/diff-test.sh"
  },
  "keywords": [
    "jsii",
    "aws"
  ],
  "dependencies": {
    "clone": "^2.1.1",
    "codemaker": "^0.5.0-beta",
    "fs-extra": "^4.0.3",
    "jsii-dotnet-generator": "^0.5.0-beta",
    "jsii-java-runtime": "^0.5.0-beta",
    "jsii-spec": "^0.5.0-beta",
    "source-map-support": "^0.5.6",
    "xmlbuilder": "^10.0.0",
    "yargs": "^9.0.0"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/fs-extra": "^4.0.8",
    "@types/node": "^9.6.18",
    "@types/nodeunit": "0.0.30",
    "@types/xmlbuilder": "^0.0.32",
    "@types/yargs": "^6.6.0",
    "jsii-build-tools": "^0.5.0-beta",
    "jsii-calc": "^0.5.0-beta",
    "@scope/jsii-calc-lib": "^0.5.0-beta",
    "nodeunit": "^0.11.2",
    "tslint": "*",
    "typescript": "^2.9.2"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/jsii"
  }
}
