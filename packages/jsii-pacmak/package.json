{
  "name": "jsii-pacmak",
  "version": "0.0.0",
  "description": "A code generation framework for jsii backend languages",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "homepage": "https://github.com/aws/jsii",
  "bugs": {
    "url": "https://github.com/aws/jsii/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/jsii-pacmak"
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii-pacmak": "bin/jsii-pacmak"
  },
  "scripts": {
    "gen": "bash generate.sh",
    "build": "yarn gen && tsc --build && chmod +x bin/jsii-pacmak && yarn lint",
    "watch": "tsc --build -w",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.ts --ignore-path=.gitignore",
    "lint:fix": "yarn lint --fix",
    "test": "yarn test:unit && yarn test:build",
    "test:unit": "jest",
    "test:build": "bash test/build-test.sh",
    "test:update": "jest -u && yarn test:build",
    "package": "package-js"
  },
  "dependencies": {
    "@jsii/check-node": "0.0.0",
    "@jsii/spec": "0.0.0",
    "clone": "^2.1.2",
    "codemaker": "^0.0.0",
    "commonmark": "^0.31.2",
    "escape-string-regexp": "^4.0.0",
    "fs-extra": "^10.1.0",
    "jsii-reflect": "^0.0.0",
    "semver": "^7.7.2",
    "spdx-license-list": "^6.10.0",
    "xmlbuilder": "^15.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jsii/dotnet-runtime": "^0.0.0",
    "@jsii/go-runtime": "^0.0.0",
    "@jsii/java-runtime": "^0.0.0",
    "@scope/jsii-calc-lib": "^0.0.0",
    "@types/clone": "^2.1.4",
    "@types/commonmark": "^0.27.10",
    "@types/diff": "^5.2.3",
    "@types/fs-extra": "^9.0.13",
    "@types/semver": "^7.7.0",
    "@types/yargs": "^17.0.33",
    "diff": "^5.2.0",
    "jsii": "^5.9.1",
    "jsii-build-tools": "^0.0.0",
    "jsii-calc": "^3.20.120",
    "jsii-rosetta": "~5.9.1",
    "pyright": "^1.1.403"
  },
  "peerDependencies": {
    "jsii-rosetta": ">=5.7.0"
  },
  "keywords": [
    "jsii",
    "aws"
  ]
}
