{
  "name": "jsii-pacmak",
  "version": "0.0.0",
  "description": "A code generation framework for jsii backend languages",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "homepage": "https://github.com/aws/jsii",
  "bugs": {
    "url": "https://github.com/aws/jsii/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/jsii-pacmak"
  },
  "engines": {
    "node": ">= 10.3.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii-pacmak": "bin/jsii-pacmak"
  },
  "scripts": {
    "gen": "bash generate.sh",
    "build": "npm run gen && tsc --build && chmod +x bin/jsii-pacmak && npm run lint",
    "watch": "tsc --build -w",
    "lint": "eslint . --ext .js,.ts --ignore-path=.gitignore",
    "lint:fix": "yarn lint --fix",
    "test": "jest && npm run test:build",
    "test:build": "bash test/build-test.sh",
    "test:update": "jest -u && npm run test:build",
    "package": "package-js"
  },
  "dependencies": {
    "@jsii/spec": "^0.0.0",
    "clone": "^2.1.2",
    "codemaker": "^0.0.0",
    "commonmark": "^0.29.1",
    "escape-string-regexp": "^4.0.0",
    "fs-extra": "^9.0.1",
    "jsii-reflect": "^0.0.0",
    "jsii-rosetta": "^0.0.0",
    "semver": "^7.3.2",
    "spdx-license-list": "^6.2.0",
    "xmlbuilder": "^15.1.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@jsii/dotnet-runtime": "^0.0.0",
    "@jsii/java-runtime": "^0.0.0",
    "@scope/jsii-calc-lib": "^0.0.0",
    "@types/clone": "^2.1.0",
    "@types/commonmark": "^0.27.4",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^26.0.9",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "^10.17.28",
    "@types/semver": "^7.3.2",
    "@types/yargs": "^15.0.5",
    "eslint": "^7.6.0",
    "jest": "^26.4.0",
    "jsii-build-tools": "^0.0.0",
    "jsii-calc": "^0.0.0",
    "mock-fs": "^4.12.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "~3.9.7"
  },
  "keywords": [
    "jsii",
    "aws"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/bin/**/*.js",
      "**/lib/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "errorOnDeprecated": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).ts"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  }
}
