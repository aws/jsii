    "runtime",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/@jsii/dotnet-runtime"
  },
  "engines": {
    "node": ">= 10.3.0"
  },
  "main": "lib/index.js",
