    "test": "/bin/bash ./test.sh",
    "test:update": "UPDATE_DIFF=1 npm run test",
    "package": "package-dotnet ./src/Amazon.JSII.Runtime.sln && package-private"
  },
  "devDependencies": {
    "@jsii/runtime": "^0.0.0",
    "@types/node": "^10.17.28",
    "@types/semver": "^7.3.1",
