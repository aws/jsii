  "description": ".NET client for jsii runtime",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "email": "aws-jsii@amazon.com"
  },
  "homepage": "https://github.com/aws/jsii",
  "bugs": {
