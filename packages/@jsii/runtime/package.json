{
  "name": "@jsii/runtime",
  "version": "0.0.0",
  "description": "jsii runtime kernel process",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "homepage": "https://github.com/aws/jsii",
  "bugs": {
    "url": "https://github.com/aws/jsii/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git",
    "directory": "packages/@jsii/runtime"
  },
  "engines": {
    "node": ">= 10.3.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jsii-runtime": "bin/jsii-runtime"
  },
  "scripts": {
    "build": "tsc --build && chmod +x bin/jsii-runtime && npm run lint && npm run bundle",
    "bundle": "ts-node build-tools/bundle.ts",
    "watch": "tsc --build -w",
    "lint": "eslint . --ext .js,.ts --ignore-path=.gitignore --ignore-pattern=webpack.config.js",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:update": "jest -u",
    "package": "package-js"
  },
  "dependencies": {
    "@jsii/kernel": "^0.0.0",
    "@jsii/spec": "^0.0.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@scope/jsii-calc-base": "^0.0.0",
    "@scope/jsii-calc-lib": "^0.0.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^10.17.55",
    "esbuild": "^0.11.2",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "jsii-build-tools": "^0.0.0",
    "jsii-calc": "^3.20.120",
    "prettier": "^2.2.1",
    "source-map-loader": "^2.0.1",
    "ts-jest": "^26.5.4",
    "typescript": "~4.2.3"
  }
}
